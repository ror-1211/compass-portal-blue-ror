<p id="notice"><%= notice %></p>

<h1>Openingtimes</h1>

<table>
  <thead>
    <tr>
      <th>Opens</th>
      <th>Closes</th>
      <th>Day</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @openingtimes.each do |openingtime| %>
      <tr>
        <td><%= openingtime.opens %></td>
        <td><%= openingtime.closes %></td>
        <td><%= openingtime.day %></td>
        <td><%= link_to 'Show', openingtime %></td>
        <td><%= link_to 'Edit', edit_openingtime_path(openingtime) %></td>
        <td><%= link_to 'Destroy', openingtime, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Openingtime', new_openingtime_path %>
